*.private.salami.network {
  tls /etc/ssl/caddy/fullchain.pem /etc/ssl/caddy/privkey.pem

  @stirlingpdf host stirlingpdf.private.salami.network
  handle @stirlingpdf {
    reverse_proxy 10.88.111.11:8080
  }

  @adguard host adguard.private.salami.network
  handle @adguard {
    reverse_proxy 10.88.111.7:80
  }
}
